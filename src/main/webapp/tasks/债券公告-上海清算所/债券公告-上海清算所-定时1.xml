		<property name="timeOut" value="60000"></property>
		<property name="useGzip" value="false"></property>
		<property name="headers">
			<map>
				<entry key="Accept-Encoding" value="deflate"/>   
			</map>
		</property>
		<property name="acceptStatCode">
