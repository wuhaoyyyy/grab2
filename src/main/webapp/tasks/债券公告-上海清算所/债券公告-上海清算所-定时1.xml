		<property name="field" value="pubdate"></property> 
		<property name="fromDateFormat" value="chinese"></property>
		<property name="toDateFormat" value="yyyyMMdd"></property>
		<property name="ruleExpression" value="$.datas[*].pubdate"></property> 
		<property name="ruleType" value="xjson"></property> 
		<property name="ruleResultType" value="2"></property> 
	</bean>
	
